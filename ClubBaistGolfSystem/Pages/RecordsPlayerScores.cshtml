@page
@model ClubBaistGolfSystem.Pages.RecordsPlayerScoresModel
@{
}
<!DOCTYPE html>
<html lang="en">

<head>
    <title>RecordsPlayerScores</title>
    <meta charset="utf-8" />
    <script type="text/javascript">

         function isValid(aForm)
         {
            var isValid = false;

             var aValue = aForm.MemberNumber.value
             var length = aValue.length;
               if (length > 0)
               {
                isValid = true;
               }
                else
               {
                alert('MemberNumber is required')
               }

              var bValue = aForm.Course.value
              var blength = bValue.length;
               if (blength > 0)
               {
                isValid = true;
               }
                else
               {
                alert('Course is required')
               }

                var cValue = aForm.Rating.value
                var clength = cValue.length;
               if (clength > 0)
               {
                isValid = true;
               }
                else
               {
                alert('Rating is required')
               }

               var dValue = aForm.Slope.value
                var dlength = dValue.length;
               if (dlength > 0)
               {
                isValid = true;
               }
                else
               {
                alert('Slope is required')
               }

             var eValue = aForm.Date.value
                var elength = eValue.length;
               if (elength > 0)
               {
                isValid = true;
               }
                else
               {
                alert('Date is required')
               }


             return isValid;
         }

    </script>
</head>

<body>
    <header>Records Player Scores</header>
    <main>

